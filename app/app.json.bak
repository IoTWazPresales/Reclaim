{
  "expo": {
    "name": "Reclaim",
    "slug": "reclaim",
    "scheme": "reclaim",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b1220"
    },
    "android": {
      "package": "com.yourcompany.reclaim",
      "permissions": [
        "POST_NOTIFICATIONS",
        "WAKE_LOCK",
        "VIBRATE",
        "INTERNET",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK",
        "VIBRATE",
        "INTERNET"
      ],
      "runtimeVersion": "1.0.0"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      [
        "react-native-health-connect",
        {
          "permissions": [
            "com.google.sleep.session",
            "com.google.sleep.stage"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "newArchEnabled": false
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a16d8e13-98f9-4c3f-a6fd-bbd16de61992"
      }
    },
    "owner": "eliasonw",
    "updates": {
      "url": "https://u.expo.dev/a16d8e13-98f9-4c3f-a6fd-bbd16de61992"
    }
  }
}
